{
  "name": "wojtek-vending-machine",
  "version": "0.1.0",
  "bin": {
    "wojtek-vending-machine": "bin/wojtek-vending-machine.js"
  },
  "scripts": {
    "dev": "tsc -w",
    "build:cdk": "node build.js -t cdk",
    "build:functions": "cd ./src && make build",
    "build": "npm run clean && npm run build:cdk && npm run build:functions",
    "test": "is-ci \"test:ci\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand --detectOpenHandles",
    "clean": "rm -rf dist cdk.out",
    "synth": "npm run build && cdk synth > /dev/null",
    "deploy": "npm run synth && cdk deploy",
    "bootstrap": "npm run synth && cdk bootstrap",
    "update": "ncu -u",
    "destroy": "cdk destroy"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.88.0",
    "@aws-cdk/aws-apigatewayv2": "1.88.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "1.88.0",
    "@aws-cdk/aws-dynamodb": "1.88.0",
    "@aws-cdk/aws-lambda": "1.88.0",
    "@aws-cdk/core": "1.88.0",
    "@types/app-root-path": "1.2.4",
    "@types/aws-lambda": "8.10.71",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.25",
    "app-root-path": "3.0.0",
    "aws-cdk": "1.88.0",
    "chalk": "4.1.0",
    "esbuild": "0.8.42",
    "fast-glob": "3.2.5",
    "is-ci": "2.0.0",
    "is-ci-cli": "2.2.0",
    "jest": "26.6.3",
    "npm-check-updates": "11.1.1",
    "source-map-support": "0.5.19",
    "ts-jest": "26.5.0",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "yargs": "16.2.0"
  },
  "dependencies": {
    "@aws-cdk/aws-cognito": "1.88.0",
    "aws-sdk": "2.839.0"
  }
}
